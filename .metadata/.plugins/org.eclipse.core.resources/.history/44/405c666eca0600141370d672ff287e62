//
public class sec7 extends GraphicsProgram {
	
	//
	public void init() {
		setSize(800, 600);
		add(new JButton("Hello"), SOUTH);
		addActionListeners();
	}
	
	public void actionPerformed(ActionEvent e) {
		String cmd = e.getActionCommand();
		
		println("hello");
	}
	
}
