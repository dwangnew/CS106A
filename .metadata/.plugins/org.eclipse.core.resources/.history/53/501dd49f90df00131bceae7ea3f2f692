public class MyProgram extends GraphicsProgram {
	
	private static final int NROWS = 8;
	private static final int NCOLUMNS = 8;
	
	public void checkboard() {
		for (int i=0;i<NCOLUMNS;i++) {
			GRect tile = new GRect(1,1,10,10);
			add(tile);
		}
	}
}