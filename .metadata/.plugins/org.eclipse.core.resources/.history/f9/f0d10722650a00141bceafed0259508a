
// section 9 #2 expandable array

import java.awt.*;
import java.util.*;

public class sec9 {
	
	public sec9(Object obj, int index) {
		arr = new Object[index];
		arr[index] = obj;
	}
	
	public Object get(int index) {
		if ( index + 1 > arr.length ) {
			return null;
		}
		else {
			return arr[index];
		}
	}
	
